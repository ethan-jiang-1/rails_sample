<!DOCTYPE html>
<html>
<head>
	<title><%= title %></title>
	<%= csrf_meta_tag %>
	<%= render 'layouts/stylesheets' %>
</head>
<body>
	<div class="container">
		<%= render 'layouts/header' %>
		
		<section class="round">
			<%= yield %>
		</section>
		
		<%= render 'layouts/footer' %>
		
		
		<nav class="round">
			<p class="debug"><%= debug(params) if Rails.env.development? %></p>
		</nav>
	</div>
</body>
</html>
